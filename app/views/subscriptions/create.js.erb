<%= render partial: 'shared/update_feeds', locals: {feed_data: @feed_data} %>
feedbin.showNotification('You have subscribed.');
feedbin.hideSubscribe();
<% @subscriptions.each do |subscription| %>
    <% if subscription.title.present? %>
        feedbin.data.user_titles[<%= subscription.feed_id %>] = '<%= j subscription.title %>';
        feedbin.applyUserTitles(false);
    <% end %>
<% end %>

var feed = $('[data-feed-id=<%= @click_feed %>] a')
feedbin.reveal(feed);

$('.modal-purpose-subscribe').modal('hide')
